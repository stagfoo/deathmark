{
  "name": "deathmark",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "watch": "webpack-dev-server --content-base dist --hot --inline --mode=development",
    "build": "webpack -p",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.stagfoo.DeathMark",
    "productName": "DeathMark",
    "mac": {
      "category": "Video",
      "icon": "favicons/icon.icns"
    },
    "win": {
      "icon": "favicons/icon.ico"
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "awesome-typescript-loader": "5.2.1",
    "create-pwa": "^2.3.0",
    "electron": "^11.5.0",
    "electron-builder": "^22.9.1",
    "source-map-loader": "0.2.4",
    "typescript": "3.0.3",
    "webpack": "^4.43.0",
    "webpack-dev-server": "^3.11.0",
    "workbox-webpack-plugin": "5.0.0"
  },
  "dependencies": {
    "electron-packager": "^15.2.0",
    "fs": "0.0.1-security",
    "lit-html": "^1.3.0",
    "lowdb": "^1.0.0",
    "morphdom": "^2.6.1",
    "nanohtml": "^1.9.1",
    "obake.js": "^1.2.1"
  }
}
